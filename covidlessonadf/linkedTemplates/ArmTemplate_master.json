{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covidlessonadf"
		},
		"ls_blob_covidlessonstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_covidlessonstorage'"
		},
		"ls_blob_udemylessondatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_blob_udemylessondatalake'"
		},
		"ls_blob_udemylessondatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://udemylessondatalake.dfs.core.windows.net/"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/1b899802-159d-4833-9968-c48a5e4b486d/resourceGroups/udemy.covid.lesson/providers/Microsoft.Storage/storageAccounts/covidlessonstorage"
		},
		"ls_edcd_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covidlessonadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blob_covidlessonstorage_connectionString": {
						"value": "[parameters('ls_blob_covidlessonstorage_connectionString')]"
					},
					"ls_blob_udemylessondatalake_accountKey": {
						"value": "[parameters('ls_blob_udemylessondatalake_accountKey')]"
					},
					"ls_blob_udemylessondatalake_properties_typeProperties_url": {
						"value": "[parameters('ls_blob_udemylessondatalake_properties_typeProperties_url')]"
					},
					"ls_edcd_properties_typeProperties_url": {
						"value": "[parameters('ls_edcd_properties_typeProperties_url')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}